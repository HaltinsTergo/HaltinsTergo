import java.util.Scanner ;

public class tunaJava {
         public static void main(String[] args) {
             Scanner input = new Scanner(System.in);

             double armut , armutTutar = 2.14 , elma , elmaTutar = 3.67 ,
                     domates , domatesTutar = 1.11 , muz ,muzTutar = 0.95 , patlican , patlicanTutar= 5.00 , toplamTutar ;

             System.out.println("Armut Kaç Kilo ?  = ");
             armut = input.nextDouble();

             System.out.println("Elma Kaç Kilo ?  =");
             elma = input.nextDouble();

             System.out.println("Domates Kaç Kilo ?  =");
             domates = input.nextDouble();

             System.out.println("Muz Kaç Kilo ?  =");
             muz = input.nextDouble();

             System.out.println("Patlıcan Kaç Kilo ?  =");
             patlican = input.nextDouble();


            toplamTutar = (armut * armutTutar + elma * elmaTutar + domates * domatesTutar + muz * muzTutar + patlican * patlicanTutar) ;

            System.out.println("Toplam Tutarınız = " + toplamTutar );



         }
